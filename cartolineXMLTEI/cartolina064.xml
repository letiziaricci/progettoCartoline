      <front xmlns="http://www.tei-c.org/ns/1.0">
         <div type="tipografia" part="N" org="uniform" sample="complete">
            <p facs="#copyright_64" xml:id="copyright-64" part="N">
               <s rend="writing-mode(vertical-lr) text-orientation(sideways)" part="N">
                              Copyright by
                              <persName ref="list.xml#AN" instant="false" full="yes">
                     <abbr>A.</abbr> Noyer
                              </persName>.
                              <date instant="false">1912</date>. – Allegorie
                              <choice>
                     <abbr>N°</abbr>
                     <expan instant="false">N<ex instant="false">umero</ex>
                     </expan>
                  </choice>
                              198
                        </s>
            </p>
         </div>
      </front>
      <body xmlns="http://www.tei-c.org/ns/1.0">
         <div type="fronte" part="N" org="uniform" sample="complete">
            <figure><!-- contiene tutta l'immagine-->
                
                  <figDesc>Un uomo bacia la mano di una donna immersa nella natura di un bosco.</figDesc>
               
            </figure>
         </div>
         <div type="retro" part="N" org="uniform" sample="complete">
            <div type="message" part="N" org="uniform" sample="complete">
               <opener>
                  <dateline>
                     <lb n="1" facs="#lineText_1-64" xml:id="lineText-1-64"/>
                     <s part="N">
                        <placeName ref="list.xml#RV" instant="false" full="yes">Ravenna</placeName>
                        <date when="1913/07/25" instant="false">25-VII-913</date>
                     </s>
                  </dateline>
               </opener>
               <div type="postcard-body" part="N" org="uniform" sample="complete">
                  <p part="N">
                     <lb n="2" facs="#lineText_2-64" xml:id="lineText-2-64"/>
                              Oggi finalmente abbiam-
                              <lb n="3" facs="#lineText_3-64" xml:id="lineText-3-64"/>
                              mo avuto una bella gior-
                              <lb n="4" facs="#lineText_4-64" xml:id="lineText-4-64"/>
                              nata piena di sole: da
                              <lb n="5" facs="#lineText_5-64" xml:id="lineText-5-64"/>
                              tanto tempo era attesa.
                              <lb n="6" facs="#lineText_6-64" xml:id="lineText-6-64"/>
                              La salute è ottima.
                              <lb n="7" facs="#lineText_7-64" xml:id="lineText-7-64"/>
                              Attendiamo notizie
                              <lb n="8" facs="#lineText_8-64" xml:id="lineText-8-64"/>
                              da Arona.
                        </p>
               </div>
               <closer>
                  <s part="N">
                     <lb n="9" facs="#lineText_9-64" xml:id="lineText-9-64"/>
                              Saluti sinceri a te e
                              <lb n="10" facs="#lineText_10-64" xml:id="lineText-10-64"/>
                              alla Sig.na Carolina.
                        </s>
                  <lb n="11" facs="#lineText_11-64" xml:id="lineText-11-64"/>
                  <signed>
                     <persName instant="false" full="yes">
                        <forename ref="list.xml#GT" full="yes">Giuseppe</forename>
                     </persName>
                  </signed>
               </closer>
            </div>
            <div type="address-destination"
                 part="N"
                 org="uniform"
                 sample="complete">
               <p part="N">
                  <address>
                     <addrLine>
                        <lb n="12" facs="#lineText_12-64" xml:id="lineText-12-64"/>
                                    Signorina
                                    <lb n="13" facs="#lineText_13-64" xml:id="lineText-13-64"/>
                                    Oliva Turtura
                                    <lb n="14" facs="#lineText_14-64" xml:id="lineText-14-64"/>
                                    presso Sig. Cibecchini
                                    <lb n="15" facs="#lineText_15-64" xml:id="lineText-15-64"/>
                                    Via Cavour 12
                                    <lb n="16" facs="#lineText_16-64" xml:id="lineText-16-64"/>
                                    Arona
                                    <lb n="17" facs="#lineText_17-64" xml:id="lineText-17-64"/>
                                    (Lago Maggiore)
                              </addrLine>
                  </address>
               </p>
            </div>
            <div type="postmark-postage"
                 part="N"
                 org="uniform"
                 sample="complete">
               <ab part="N">
                  <stamp type="postage"
                         rend="rectangular"
                         facs="#postage_11"
                         xml:id="postage-11">
                     <mentioned rend="uppercase">Poste Italiane 10 centesimi</mentioned>
                  </stamp>
                  <stamp type="postmark"
                         rend="circular"
                         facs="#stamp_left-64"
                         xml:id="stamp-left-64">
                     <mentioned>
                        <unclear reason="faded" instant="false">
                           <placeName ref="list.xml#RV" rend="uppercase" instant="false" full="yes">Ravenna</placeName>
                        </unclear>
                        <date when="1913-07-25" instant="false"> 25.7.13</date>.20
                     </mentioned>
                  </stamp>
                  <stamp type="postmark"
                         rend="circular"
                         facs="#stamp_right-64"
                         xml:id="stamp-right-64">
                     <mentioned>
                        <placeName ref="list.xml#RV" rend="uppercase" instant="false" full="yes">Ravenna</placeName>
                        <date when="1913-07-25" instant="false"> 25.7.13</date>.20
                     </mentioned>
                  </stamp>
                  <stamp type="postmark"
                         rend="circular"
                         facs="#stamp_middle-64"
                         xml:id="stamp-middle-64">
                     <mentioned>
                        <placeName ref="list.xml#AR" rend="uppercase" instant="false" full="yes">Arona</placeName>
                        <date when="1913-07-25" instant="false">
                           <supplied resp="list.xml#pLR" cert="good" instant="false">
                              <unclear reason="faded" instant="false">28</unclear>
                           </supplied>7.13
                              </date>.10
                        </mentioned>
                  </stamp>
               </ab>
            </div>
            <div type="secondamano" part="N" org="uniform" sample="complete">
               <ab type="paratext" part="N">
                  <add facs="#number64_64"
                       xml:id="number64-64"
                       instant="false"
                       status="unremarkable">
                     <num value="64">64</num>
                  </add>
               </ab>
            </div>
         </div>
